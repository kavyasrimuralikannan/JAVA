class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int count=0;
        int numberOfOnes=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]==1){
                numberOfOnes++;
                count=Math.max(numberOfOnes,count);
            }
            else numberOfOnes=0;
        }
        return count;
    }
}
